{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "save tabs in timestamped bookmark folder"
  },
  "description": "save tabs in timestamped bookmark folder",
  "icons": {
	  "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "save-tabs-to-bookmark-folder",
  "permissions": [
    "menus",
    "tabs",
    "bookmarks",
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+S"
      }
    }
  },
  "version": "1.2.6"
}
