{
  "manifest_version": 2,

  "name": "snapshot tabs as bookmarks",
  "description": "saves open tabs into a timestamped bookmark folder",
  "version": "1.0.0",

  "browser_action": {
    "default_title": "Bookmark Tabs"
  },
  "permissions": [
    "activeTab","tabs","bookmarks"
  ],
  "background": {
    "scripts": ["background.js"]    
  }
}
