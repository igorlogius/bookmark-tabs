{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/bookmark-tabs",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "description": "Saves selected or all tabs into a timestamped bookmark folder for later use",
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Bookmark Tabs",
  "permissions": ["tabs", "bookmarks", "storage", "notifications"],
  "options_ui": {
    "page": "options.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+S"
      }
    }
  },
  "default_area": "navbar",
  "version": "1.5.12"
}
