{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Save Tabs to Bookmark Folder"
  },
  "description": "Save tabs to a timestamped bookmark folder",
  "icons": {
	  "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Save Tabs to Bookmark Folder",
  "permissions": [
    "menus",
    "tabs",
    "bookmarks",
    "storage",
    "notifications"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+S"
      }
    }
  },
  "version": "1.4.8"
}
